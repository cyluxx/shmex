{
  "name": "shmex",
  "version": "0.5.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "clean": "rimraf ./dist",
    "e2e": "ng e2e",
    "format:all": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "ng": "ng",
    "postversion": "git push && git push --tags",
    "precommit": "npm run format:fix && npm run lint",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "version": "git add -A src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.1",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.1",
    "@angular/compiler": "~10.1.1",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~10.1.1",
    "@angular/localize": "^10.1.1",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.1.1",
    "@angular/platform-browser-dynamic": "~10.1.1",
    "@angular/router": "~10.1.1",
    "@angular/service-worker": "~10.1.1",
    "@ctrl/ngx-codemirror": "^3.1.3",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "bootstrap": "^4.5.2",
    "codemirror": "^5.53.2",
    "file-saver": "^2.0.2",
    "fraction.js": "^4.0.12",
    "opensheetmusicdisplay": "^0.8.7",
    "osmd-audio-player": "^0.6.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "vexflow": "^1.2.87",
    "xml-formatter": "^2.1.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "~10.1.1",
    "@angular/compiler-cli": "~10.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "protractor": "~7.0.0",
    "puppeteer": "^3.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6"
  }
}
